<h4>News Feed</h4>
<hr>
<% if @posts.empty? %>
  <h4>Subscribe on some Tags to have own feed</h4>
  <% if user_signed_in? %>
    <div class="fixed-action-btn horizontal add_button">
      <%= link_to new_post_path, class: 'btn-floating btn-large red' do %>
        <i class="large material-icons">mode_edit</i>
      <% end %>
    </div>
  <% end %>
<% else %>
  <%= render 'mainpage' %>
  <div class="row">
    <div class="col s12">
      <br><br><br>
      <%= paginate @posts %>
    </div>
  </div>
<% end %>


