<% if current_page.first? %>
  <li class="ddisabled">
    <%= content_tag :a do %>
      <i class="material-icons">chevron_left</i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect">
    <%= link_to url, :rel => 'pprev', :remote => remote do %>
      <i class="material-icons">chevron_left</i>
    <% end %>
  </li>
<% end %>
